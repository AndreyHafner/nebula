<target name="allElements">
  <ant antfile="${genericTargets}" target="${target}" >
 <property name="type" value="feature" />
  <property name="id" value="org.eclipse.nebula.feature" />
 </ant>
</target>
<target name="getMapFiles">
    <property name="cvsRoot" value=":pserver:anonymous@dev.eclipse.org:/cvsroot/technology" />
  <cvs cvsroot="${cvsRoot}"
    dest="${buildDirectory}/maps"
   command="export -r ${mapVersionTag} org.eclipse.swt.nebula/org.eclipse.nebula.feature/nebula.map"/>
</target>
<target name="assemble.org.eclipse.nebula.feature">
  <ant antfile="${assembleScriptName}" dir="${buildDirectory}">
 <property name="zipargs" value="" />
  </ant>
</target>
